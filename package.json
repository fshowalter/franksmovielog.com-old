{
  "name": "movielog.frankshowalter.com",
  "version": "1.0.0",
  "description": "movielog.frankshowalter.com",
  "author": "Frank Showalter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fshowalter/franksmovielog.com/issues"
  },
  "homepage": "https://github.com/fshowalter/franksmovielog.com#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fshowalter/franksmovielog.com.git"
  },
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "node-sass src/site/_includes/css/ --dir src/site/css/",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "postcss --watch src/site/_includes/css/ --dir src/site/css/",
    "watch:html": "eleventy --serve",
    "start": "yarn watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.0",
    "markdown-it": "^11.0.0",
    "markdown-it-footnote": "^3.0.2",
    "moment": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "prettier-plugin-html-template-literals": "^1.0.5",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1"
  }
}
