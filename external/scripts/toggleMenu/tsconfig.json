{
  "compilerOptions": {
    "module": "None",
    "target": "esnext",
    "lib": ["dom", "es2017"],
    "strict": true,
    "allowJs": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "outFile": "../../../static/scripts/toggleMenu.js"
  }
}
